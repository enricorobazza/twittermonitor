server {
    listen 2195;
    server_name andromeda.lasdpc.icmc.usp.br;

    location / {
        proxy_pass http://127.0.0.1:2196;
        proxy_set_header Host $host;
        proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    }
    location /static {
        root /opt/app/twitterkafka;
    }
}